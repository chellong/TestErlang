-module(lib_misc).

%% ====================================================================
%% API functions
%% ====================================================================
-export([]).

%% ====================================================================
%% Internal functions
%% ====================================================================

for(Max,Max,F)	->
	[F(Max)];
for(I.MAX,F) ->
	[F(I)|for(I + 1),Max,F].

qsort([]) ->
	[];

