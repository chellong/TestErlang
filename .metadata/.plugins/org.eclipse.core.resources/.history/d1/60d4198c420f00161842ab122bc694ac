%% @doc @todo Add description to lib_misc.


-module(lib_misc).

%% ====================================================================
%% API functions
%% ====================================================================
-export([]).

%% ====================================================================
%% Internal functions
%% ====================================================================

for(Max,Max,F)	->
	[F(Max)];
for(I,Max,F) ->
	[F(I)|for((I+1),Max,F)].

qsort([]) ->
	[];
qsort([Pivot|T]) ->
	qsort([X || X <- T , T < Pivot])


